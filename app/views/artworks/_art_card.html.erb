<div class="art-card" id='art-card-<%= artwork.id %>' data-artwork-id='<%= artwork.id %>'>
  <div class='art-card-container'><h5 class='score'>Score:  <%= score (artwork) %> </h5>
    <%= magnific_img_tag (artwork) %>
    <h4><%= artwork.title %></h4>
    <h5><%= artwork.artist %></h5>
    <%= thumbs_up (artwork) %>
    <%= thumbs_down (artwork) %>
    <%= link_to 'Show', artwork, class: 'btn btn-sm btn-primary' %>
    <%= link_to 'Edit', edit_artwork_path(artwork), class: 'btn btn-sm btn-primary' %>
    <%= link_to 'Destroy', artwork, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-sm btn-danger' %>
  </div>
</div>